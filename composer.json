{
  "name": "lctrs/php-ext-stubs-generator",
  "description": "Generate stubs for any PHP extension.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jérôme Parmentier",
      "email": "jerome@prmntr.me"
    }
  ],
  "homepage": "https://github.com/Lctrs/php-ext-stubs-generator",
  "support": {
    "issues": "https://github.com/Lctrs/php-ext-stubs-generator/issues",
    "source": "https://github.com/Lctrs/php-ext-stubs-generator"
  },
  "require": {
    "php": ">=8.0.12 <8.2.0",
    "nikic/php-parser": "^4.13.2",
    "nyholm/sunflower": "^0.2.1",
    "roave/better-reflection": "^5.2.0",
    "symfony/console": "^6.0.2",
    "symfony/runtime": "^6.0.5",
    "thecodingmachine/safe": "^1.3.3"
  },
  "require-dev": {
    "doctrine/coding-standard": "^9.0.0",
    "ergebnis/composer-normalize": "^2.25.1",
    "ergebnis/license": "^1.2.0",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.5.4",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.1.0",
    "phpstan/phpstan-strict-rules": "^1.1.0",
    "phpstan/phpstan-symfony": "^1.1.8",
    "phpunit/phpunit": "^9.5.20",
    "psalm/plugin-phpunit": "^0.16.1",
    "psalm/plugin-symfony": "^3.1.3",
    "symplify/config-transformer": "^10.1.4",
    "thecodingmachine/phpstan-safe-rule": "^1.2.0",
    "vimeo/psalm": "^4.22"
  },
  "autoload": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/generate-stubs-for-ext"
  ],
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "symfony/runtime": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
