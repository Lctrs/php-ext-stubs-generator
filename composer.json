{
  "name": "lctrs/php-ext-stubs-generator",
  "description": "Generate stubs for any PHP extension.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jérôme Parmentier",
      "email": "jerome@prmntr.me"
    }
  ],
  "homepage": "https://github.com/Lctrs/php-ext-stubs-generator",
  "support": {
    "issues": "https://github.com/Lctrs/php-ext-stubs-generator/issues",
    "source": "https://github.com/Lctrs/php-ext-stubs-generator"
  },
  "require": {
    "php": ">=8.0.12 <8.2.0",
    "nikic/php-parser": "^4.15.3",
    "nyholm/sunflower": "^0.2.1",
    "roave/better-reflection": "^5.11.0",
    "symfony/console": "^6.0.19",
    "symfony/runtime": "^6.0.19",
    "thecodingmachine/safe": "^2.4.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "9.0.0",
    "ergebnis/composer-normalize": "2.30.2",
    "ergebnis/license": "2.1.0",
    "phpstan/extension-installer": "1.2.0",
    "phpstan/phpstan": "1.10.9",
    "phpstan/phpstan-deprecation-rules": "1.1.3",
    "phpstan/phpstan-phpunit": "1.3.11",
    "phpstan/phpstan-strict-rules": "1.5.0",
    "phpstan/phpstan-symfony": "1.2.23",
    "phpunit/phpunit": "9.6.6",
    "psalm/plugin-phpunit": "0.18.4",
    "psalm/plugin-symfony": "4.0.3",
    "symplify/config-transformer": "11.1.27.72",
    "thecodingmachine/phpstan-safe-rule": "1.2.0",
    "vimeo/psalm": "4.30.0"
  },
  "autoload": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/generate-stubs-for-ext"
  ],
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "symfony/runtime": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
