{
  "name": "lctrs/php-ext-stubs-generator",
  "description": "Generate stubs for any PHP extension.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Jérôme Parmentier",
      "email": "jerome@prmntr.me"
    }
  ],
  "homepage": "https://github.com/Lctrs/php-ext-stubs-generator",
  "support": {
    "issues": "https://github.com/Lctrs/php-ext-stubs-generator/issues",
    "source": "https://github.com/Lctrs/php-ext-stubs-generator"
  },
  "require": {
    "php": ">=8.0.12 <8.2.0",
    "nikic/php-parser": "^4.15.1",
    "nyholm/sunflower": "^0.2.1",
    "roave/better-reflection": "^5.11.0",
    "symfony/console": "^6.0.13",
    "symfony/runtime": "^6.0.11",
    "thecodingmachine/safe": "^2.4.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "9.0.0",
    "ergebnis/composer-normalize": "2.28.3",
    "ergebnis/license": "2.0.0",
    "phpstan/extension-installer": "1.2.0",
    "phpstan/phpstan": "1.8.11",
    "phpstan/phpstan-deprecation-rules": "1.0.0",
    "phpstan/phpstan-phpunit": "1.1.3",
    "phpstan/phpstan-strict-rules": "1.4.4",
    "phpstan/phpstan-symfony": "1.2.14",
    "phpunit/phpunit": "9.5.25",
    "psalm/plugin-phpunit": "0.17.0",
    "psalm/plugin-symfony": "3.1.10",
    "symplify/config-transformer": "11.1.16",
    "thecodingmachine/phpstan-safe-rule": "1.2.0",
    "vimeo/psalm": "4.29.0"
  },
  "autoload": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lctrs\\PhpExtStubsGenerator\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/generate-stubs-for-ext"
  ],
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "symfony/runtime": true
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
